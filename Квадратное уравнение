print('ax', chr(178), ' + bx + c = 0', sep='')
a = float(input('a = '))
b = float(input('b = '))
c = float(input('c = '))
D = b ** 2 - 4 * a * c
print('D =', D)
if D > 0:
    print('x1 =', (-b - D ** 0.5) / (2 * a))
    print('x2 =', (-b + D ** 0.5) / (2 * a))
elif D == 0:
    print('x =', -b / (2 * a))
else:
    print('No roots')
